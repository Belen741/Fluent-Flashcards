{
  "installCommand": "npm install",
  "buildCommand": "npx vite build",
  "outputDirectory": "dist/public",
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node@3.0.0"
    },
    "api/**/*.ts": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
